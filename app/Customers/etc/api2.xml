	<config>
		<api2>
			<resource_groups>
				<customers translate="title" module="api2">
					<title>Customers</title>
					<sort_order>30</sort_order>
					<children>
						<customers_customers translate="title" module="api2">
							<title>Customers</title>
							<sort_order>10</sort_order>
						</customers_customers>
					</children>
				</customers>
			</resource_groups>
			<resources>
				<customers translate="title" module="api2">
					<group>customers_customers</group>
					<model>compucom_customers/api2_customers</model>
					<working_model>customers/customers</working_model>
					<title>Customers Customers</title>
					<sort_order>10</sort_order>
					<privileges>
						<admin>
							<create>1</create>
							<retrieve>1</retrieve>
							<update>1</update>
							<delete>1</delete>
						</admin>
						<customers>
							<retrieve>1</retrieve>
						</customers>
						<guest>
							<retrieve>1</retrieve>
						</guest>
					</privileges>
					<attributes>
						<firstname>Firstname</firstname>
                    	<lastname>Lastname</lastname>
                    	<website_store_id>Website Store Id</website_store_id>
                    	<group_id>Group Id</group_id>
                    	<user_type>User Type</user_type>
                    	<liferay_userid>LiferayUserId </liferay_userid>
                    	<telephone>Telephone</telephone>
                    	<locale_cd>Locale_CD</locale_cd>
                    	<enable_sms>Enable SMS</enable_sms>
                    	<dob>DOB</dob>
                    	<gender>Gender</gender>
					</attributes>
					<exclude_attributes>
						
					</exclude_attributes>
					<routes>
						<route_entity>
							<route>/customers/id/:id</route>
							<action_type>entity</action_type>
						</route_entity>
						<route_entity_id>
							<route>/customers/profile/:profile_id</route>
							<action_type>entity</action_type>
						</route_entity_id>
						<route_entity_customers_shipping>
							<route>/customers/shipping/:shipping_id</route>
							<action_type>entity</action_type>
						</route_entity_customers_shipping>
						<route_entity_customers_billing>
							<route>/customers/billing/:billing_id</route>
							<action_type>entity</action_type>
						</route_entity_customers_billing>
					</routes>
					<versions>1</versions>
				</customers>
			</resources>
		</api2>
	</config>


